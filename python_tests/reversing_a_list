"""

"""
SMARTER WAY
"""
list = [4, 5, 6, 7, 8, 9]
print("Starting list:" + str(list))
def reverse_list(list):
#   built in reverse function will also show a list in reverse for you.
#   list = list.reverse()
    list = list[::-1]
    return list

a=reverse_list([4, 5, 6, 7, 8, 9])
print(a)
